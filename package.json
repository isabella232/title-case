{
  "name": "@mapbox/title-case",
  "version": "1.2.0",
  "description": "configuration for multilingual title case handling",
  "main": "index.js",
  "scripts": {
    "test": "eslint index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapbox/title-case.git"
  },
  "author": "Tom Lee <tlee@mapbox.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mapbox/title-case/issues"
  },
  "homepage": "https://github.com/mapbox/title-case#readme",
  "eslintConfig": {
    "rules": {
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "space-before-function-paren": [
        2,
        "never"
      ],
      "space-in-parens": 2,
      "space-before-blocks": 2,
      "comma-style": 2,
      "no-lonely-if": 2,
      "no-else-return": 0,
      "new-cap": 0,
      "no-empty": 2,
      "no-new": 2,
      "no-multi-spaces": 0,
      "space-in-brackets": 0,
      "brace-style": 0,
      "quotes": 0,
      "no-underscore-dangle": 0,
      "curly": 0,
      "no-constant-condition": 0,
      "no-native-reassign": 0,
      "no-shadow": 0,
      "key-spacing": 0
    },
    "env": {
      "node": true,
      "es6": true
    }
  }
}
